{
  "name": "chimata-shop",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-proxy": "node --max-http-header-size=80000 ./node_modules/@angular/cli/bin/ng serve --liveReload=false --proxyConfig=proxy.conf.js",
    "start-proxy-local": "node --max-http-header-size=80000 ./node_modules/@angular/cli/bin/ng serve --liveReload=false --proxyConfig=proxy.conf.local.js",
    "test": "ng test",
    "test-headless": "ng test --codeCoverage=true --sourceMap=true --watch=false --progress=false --browsers=ChromeNoSandboxHeadless",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build --progress=false",
    "build-prod": "ng build --prod --progress=false",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "analyze-prod": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.1.0",
    "@angular/animations": "9.1.7",
    "@angular/cdk": "10.0.2",
    "@angular/common": "9.1.7",
    "@angular/compiler": "9.1.7",
    "@angular/core": "9.1.7",
    "@angular/flex-layout": "9.0.0-beta.30",
    "@angular/forms": "9.1.7",
    "@angular/material": "9.2.3",
    "@angular/platform-browser": "9.1.7",
    "@angular/platform-browser-dynamic": "9.1.7",
    "@angular/router": "9.1.7",
    "@sentry/browser": "^5.9.0",
    "ajv": "^6.5.0",
    "blob-util": "2.0.2",
    "codelyzer": "^5.1.2",
    "core-js": "2.5.4",
    "custom-event": "1.0.1",
    "js-sha256": "0.9.0",
    "keycloak-js": "6.0.1",
    "lodash-es": "^4.17.3",
    "moment": "2.22.2",
    "popper.js": "^1.14.3",
    "prettysize": "0.0.3",
    "rxjs": "6.5.5",
    "ts-helpers": "^1.1.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "9.1.6",
    "@angular/compiler-cli": "9.1.7",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^3.5.0",
    "@types/lodash-es": "4.17.0",
    "@types/moment": "2.13.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ng-bullet": "^1.0.3",
    "prettier": "1.18.2",
    "protractor": "5.4.0",
    "ts-node": "7.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
