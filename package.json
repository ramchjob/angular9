{
  "name": "chimata-shop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-proxy": "node --max-http-header-size=80000 ./node_modules/@angular/cli/bin/ng serve --liveReload=false --proxyConfig=proxy.conf.js",
    "start-proxy-local": "node --max-http-header-size=80000 ./node_modules/@angular/cli/bin/ng serve --liveReload=false --proxyConfig=proxy.conf.local.js",
    "test": "ng test",
    "test-headless": "ng test --codeCoverage=true --sourceMap=true --watch=false --progress=false --browsers=ChromeNoSandboxHeadless",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build --progress=false",
    "build-prod": "ng build --prod --progress=false",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "analyze-prod": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
